{
  "manifest_version": 3,
  "name": "여러 단어 찾기",
  "version": "1.6.0",
  "description": "웹 페이지에서 여러 단어를 하이라이팅합니다.",
  "icons": {
    "32": "./imgs/fmw-icon-32.png",
    "64": "./imgs/fmw-icon-64.png",
    "128": "./imgs/fmw-icon-128.png",
    "256": "./imgs/fmw-icon-256.png"
  },
  "sidebar_action": {
    "default_page": "./index.html",
    "default_icon": {
      "32": "./imgs/fmw-icon-32.png",
      "64": "./imgs/fmw-icon-64.png",
      "128": "./imgs/fmw-icon-128.png",
      "256": "./imgs/fmw-icon-256.png"
    },
    "default_title": "단어 찾기",
    "use_navigation_bar": false
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
