{
  "manifest_version": 2,
  "name": "여러 단어 찾기",
  "version": "1.0.0",
  "description": "문서에서 여러 단어를 찾습니다.",
  "sidebar_action": {
    "default_page": "index.html",
    "default_icon": {
      "32": "imgs/fmw-icon-32.png"
    },
    "default_title": "여러 단어 찾기",
    "use_navigation_bar": false
  },
  "permissions": [
    "<all_urls>",
    "activeTab"
  ],
  "commands": {
    "open-sidebar": {
      "suggested_key": {
        "default": "Alt+F",
        "mac": "Alt+F"
      },
      "description": "사이드바 On/Off 단축키"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}